package logica;


public class Visita {
	
	private String fecha;
	private String cod;
	private Inmueble inm;
	private Asesor asesor;
	private Cliente cliente;
	private Oferta oferta;


	public Visita (String cod, String fecha, Inmueble inm, Asesor asesor, Cliente cliente) {
		this.cod = cod; this.fecha = fecha; this.inm = inm; this.asesor = asesor; this.cliente = cliente;
	}

	public String getCod() {
		return cod;
	}

	public void setCod(String cod) {
		this.cod = cod;
	}
	
	
	public void setFecha(String fecha) {
		this.fecha = fecha;
	}

	public String getFecha() {
		return fecha;
	}

	public Inmueble getInm() {
		return inm;
	}

	public void setInm(Inmueble inm) {
		this.inm = inm;
	}

	public Cliente getCliente() {
		return cliente;
	}

	public void setCliente(Cliente cliente) {
		this.cliente = cliente;
	}

	public Asesor getAsesor() {
		return asesor;
	}

	public void setAsesor(Asesor asesor) {
		this.asesor = asesor;
	}


	public Oferta getOferta() {
		return oferta;
	}

	public void setOferta(Oferta oferta) {
		this.oferta = oferta;
	}
	
	//Método toString sobreescrito.
	public String toString() {
		return "Visita de código: "+getCod();
	}

	
}//Fin de la clase Visita
