package logica;

public class Interesado {

	private String precioMaxCompra;
	private String precioMaxAlquiler;
	private ZonaGeografica zonaGeografica;
	private Cliente cliente;

	public Interesado(String unPrecioMaxCompra, String unPrecioMaxAlquiler) {
		precioMaxCompra = unPrecioMaxCompra;
		precioMaxAlquiler = unPrecioMaxAlquiler;
	}

	public void setPrecioMaxCompra(String pmc) {
		precioMaxCompra = pmc;
	}

	public String getPrecioMaxCompra() {
		return precioMaxCompra;
	}

	public void setPrecioMaxAlquiler(String pma) {
		precioMaxAlquiler = pma;
	}

	public String getPrecioMaxAlquiler() {
		return precioMaxAlquiler;
	}

	public void setZonaGeografica(ZonaGeografica zg) {
		this.zonaGeografica = zg;
	}

	public ZonaGeografica getZonaGeografica() {
		return zonaGeografica;
	}

	public Cliente getCliente() {
		return cliente;
	}

	public void setCliente(Cliente c) {
		cliente = c;
	}
}// Fin de la clase.
