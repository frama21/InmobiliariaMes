package logica;

public class Hipoteca {
	private String cantPendiente;
	private String banco;

	public Hipoteca(String unaCantPendiente, String unBanco) {
		cantPendiente = unaCantPendiente;
		banco = unBanco;
	}

	public void setCantidadPendiente(String cp) {
		cantPendiente = cp;
	}

	public String getCantidadPendiente() {
		return cantPendiente;
	}

	public void setBanco(String b) {
		banco = b;
	}

	public String getBanco() {
		return banco;
	}
}// Fin de la clase.
